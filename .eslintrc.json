{
  "extends": ["eslint:recommended", "prettier"],
  "env": {
    "browser": true
  },
  "globals": {
    "$": true
  },
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint/eslint-plugin"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "no-unused-vars": "off"
  }
}
