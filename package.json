{
  "name": "jquery-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "yarn dev",
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint ./src --cache --fix --ext .ts,.tsx",
    "ts": "tsc --noEmit",
    "pretest": "yarn lint",
    "prepare": "husky install",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,css,md}\"",
    "posttest": "yarn format"
  },
  "lint-staged": {
    "./src/*.ts": "eslint --cache --fix",
    "./src/*.{ts,css,md}": "prettier --write"
  },
  "devDependencies": {
    "@rollup/plugin-strip": "2.1.0",
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/jquery": "3.5.14",
    "@types/node": "17.0.30",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "cssnano": "5.1.7",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "7.0.4",
    "lint-staged": "12.4.1",
    "prettier": "2.6.2",
    "typescript": "4.6.4",
    "vite": "2.9.6",
    "vite-plugin-imagemin": "0.6.1",
    "vite-tsconfig-paths": "3.4.1"
  },
  "dependencies": {
    "autoprefixer": "10.4.5",
    "jquery": "3.6.0",
    "postcss": "8.4.12",
    "tailwindcss": "3.0.24"
  }
}
